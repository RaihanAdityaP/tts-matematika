{
  "expo": {
    "name": "TTS Matematika",
    "slug": "tts-matematika",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2196F3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ttsmatematika.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2196F3"
      },
      "package": "com.ttsmatematika.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "tts-matematika",
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font"
    ]
  }
}
